{
  "name": "@codesuperman/project-generator",
  "version": "1.0.3",
  "main": "dist/index.js",
  "license": "MIT",
  "bin": {
    "project-generator": "./bin/cli.js",
    "pg": "./bin/cli.js"
  },
  "scripts": {
    "build": "npm run clean && tsc --build && npm run copy",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "copy": "cp -r ./src/templates ./dist",
    "check": "tsc --noEmit",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check src/**/*.ts",
    "format:fix": "prettier --write .",
    "prepublish": "npm run build",
    "release": "release-it",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release:minor": "release-it --minor",
    "release:major": "release-it --major"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jsonfile": "^6.0.1",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.7.5",
    "@types/prettier": "^2.3.2",
    "commander": "^8.1.0",
    "consola": "^2.15.3",
    "pinst": "^2.1.6",
    "release-it": "^14.11.5",
    "rimraf": "^3.0.2",
    "super-cli": "^1.0.1"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "import-local": "^3.0.2",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.4.1",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
